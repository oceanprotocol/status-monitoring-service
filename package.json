{
  "name": "ocean-ens-proxy",
  "description": "Ocean Protocol ENS Proxy Server",
  "version": "0.0.0",
  "author": "Ocean Protocol <devops@oceanprotocol.com>",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .ts --ext .tsx . && npm run type-check",
    "clean": "rm -rf ./dist",
    "format": "prettier --ignore-path .gitignore './**/*.{css,yml,js,ts,tsx,json}' --write",
    "test:format": "npm run format && npm run lint",
    "test": "mocha --config=test/.mocharc.json --node-env=test --exit 'test/**/*.test.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "ethers": "^5.7.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vercel/node": "^2.5.8",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "4.7.3"
  }
}
